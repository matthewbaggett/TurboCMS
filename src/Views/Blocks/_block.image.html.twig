{% if block.getDataElements.Image %}
    <img
        {% if block.getDataElements.Image.URL %}
            src="{{ block.getDataElements.Image.URL }}"
        {% elseif block.getDataElements.Image.File %}
            src="/image/940x/{{ block.getDataElements.Image.File }}"
        {% endif %}
        {% if block.getDataElements.AltText %}
            alt="{{ block.getDataElements.AltText }}"
        {% endif %}
        />
{% endif %}
