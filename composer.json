{
  "name": "turbocms/turbocms",
  "description": "Matt's Modernish CMS",
  "require": {
    "php": "^7.0",
    "segura/appcore": "dev-master",
    "slim/http-cache": "^0.3.0",
    "imagine/imagine": "^0.6.3",
    "pekkis/mime-types": "^1.0",
    "league/flysystem": "^1.0",
    "league/flysystem-ziparchive": "^1.0",
    "nette/mail": "^2.4.0",
    "tedivm/fetch": "^0.7.0",
    "miljar/php-exif": "^0.6.3",
    "phpexiftool/exiftool": "^10.16",
    "dflydev/fig-cookies": "^1.0.2",
    "kint-php/kint-twig": "^1.0",
    "gone.io/twig-extension-gravatar": "dev-master",
    "gone.io/twig-extension-inflect": "^1.0",
    "gone.io/twig-extension-transform": "^1.0"
