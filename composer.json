{
  "name": "turbocms/turbocms",
  "description": "Matt's Modernish CMS",
  "require": {
    "php": "^7.0",
    "segura/appcore": "dev-master",
    "imagine/imagine": "^0.6.3",
    "pekkis/mime-types": "^1.0",
    "league/flysystem": "^1.0",
    "league/flysystem-ziparchive": "^1.0",
    "nette/mail": "^2.4.0",
    "tedivm/fetch": "^0.7.0",
    "miljar/php-exif": "^0.6.3",
    "phpexiftool/exiftool": "^10.16"
  },
  "bin": [
    "bin/data-importer",
    "bin/data-purge",
    "bin/data-rebuild",
    "bin/mail-fetch"
  ],
  "autoload": {
    "psr-4": {
      "TurboCMS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TurboCMS\\Test\\": "tests/"
    }
  }
}
